{
	"compilerOptions": {
	  "declaration": true,
	  "declarationDir": "./types",
	  "outDir": "./dist",
	  "rootDir": "./src",
	  "module": "ESNext",
	  "target": "ESNext",
	  "moduleResolution": "Bundler",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true
	},
	"include": [
		"src/**/*",
	],
}